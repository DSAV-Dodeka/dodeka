# This .env file contains environment variables used during dev deployment

# The docker compose project name
KV_IMAGE=~spwn@image_name@~
# Docker tag version
KV_VERSION=latest
KV_HOST_HOST=~spwn@host_host@~
# The host port will be the port that the host will open, i.e. what nginx will connect to. Hostname is localhost by default
KV_HOST_PORT=~spwn@host_port@~
# The container port is the port the container will open, which will be mapped to the HOST_PORT.
KV_CONTAINER_PORT=~spwn@container_port@~
# The docker compose project name
KV_COMPOSE_PROJECT_NAME=~spwn@project@~
NETWORK_NAME=~spwn@network_name@~