# The docker compose project name
SERVER_IMAGE={{ server.image_name }}
# Docker tag version
SERVER_VERSION=latest
SERVER_HOST_HOST={{ server.host_host }}
# The host port will be the port that the host will open, i.e. what nginx will connect to. Hostname defined by HOST_HOST
SERVER_HOST_PORT={{ server.host_port }}
# The container port is the port the container will open, which will be mapped to the HOST_PORT. Must match entrypoint.
SERVER_CONTAINER_PORT={{ server.container_port }}
DB_HOST={{ db.container_name }}
DB_PORT={{ db.container_port }}
KV_HOST={{ kv.container_name }}
KV_PORT={{ kv.container_port }}
SERVER_COMPOSE_PROJECT_NAME={{ server.project }}
NETWORK_NAME={{ main.docker_net_name }}