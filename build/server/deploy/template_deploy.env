# The docker compose project name
SERVER_IMAGE=~spwn@image_name@~
# Docker tag version
SERVER_VERSION=latest
SERVER_HOST_HOST=~spwn@host_host@~
# The host port will be the port that the host will open, i.e. what nginx will connect to. Hostname defined by HOST_HOST
SERVER_HOST_PORT=~spwn@host_port@~
# The container port is the port the container will open, which will be mapped to the HOST_PORT. Must match entrypoint.
SERVER_CONTAINER_PORT=~spwn@container_port@~
DB_HOST=~spwn@db_host@~
DB_PORT=~spwn@db_port@~
KV_HOST=~spwn@kv_host@~
KV_PORT=~spwn@kv_port@~
SERVER_COMPOSE_PROJECT_NAME=~spwn@project@~
NETWORK_NAME=~spwn@network_name@~