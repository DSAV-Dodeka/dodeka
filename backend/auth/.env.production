# Database configuration
FAROE_DB_PATH=./production.sqlite

# Server configuration
FAROE_PORT=3877

# User action invocation client
FAROE_USER_ACTION_INVOCATION_URL=http://localhost:8100/private/invoke_user_action

# Session configuration
# FAROE_SESSION_EXPIRATION: Absolute expiration time for sessions (default: 2160h = 90 days)
# Examples: 720h (30 days), 2160h (90 days), 4320h (180 days)
# FAROE_SESSION_EXPIRATION=2160h

# CORS configuration
FAROE_CORS_ALLOW_ORIGIN=https://dsavdodeka.nl
