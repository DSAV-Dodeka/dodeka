# Auth server settings for demo environment

# Database configuration
FAROE_DB_PATH=./envs/demo/db.sqlite

# Server configuration
FAROE_PORT=12770

# Private port for Python backend communication
FAROE_PRIVATE_PORT=12790

# Session configuration
# FAROE_SESSION_EXPIRATION: Absolute expiration time for sessions (default: 2160h = 90 days)
# Examples: 720h (30 days), 2160h (90 days), 4320h (180 days)
# FAROE_SESSION_EXPIRATION=2160h
#
# CORS configuration
FAROE_CORS_ALLOW_ORIGIN=https://demo.dsavdodeka.nl
