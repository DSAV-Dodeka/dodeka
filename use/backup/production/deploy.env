# This .env file contains environment variables used during deployment

# The tag to upload to GHCR to
BACKUP_APP_IMAGE=ghcr.io/dsav-dodeka/barman
# Docker tag version
# RECOMMENDED TO WRAP
BACKUP_VERSION=latest

BARMAN_PASSWORD=

# This is the directory that the resources will be loaded into in the container
BACKUP_RESOURCES_TARGET=/dodeka_barman
# This is the directory that will be used for recovery
BACKUP_RECOVER_TARGET=/dodeka_recover
# The docker compose project name, influences the container name
BACKUP_COMPOSE_PROJECT_NAME=b-dodeka
NETWORK_NAME=dodeka